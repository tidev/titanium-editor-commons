{
  "name": "titanium-editor-commons",
  "version": "2.1.0",
  "description": "",
  "main": "index.js",
  "author": "Axway, Inc. <npmjs@appcelerator.com>",
  "maintainers": [
    "Ewan Harris <eharris@axway.com>"
  ],
  "license": "Apache-2.0",
  "lint-staged": {
    "*.ts": "npm run lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "commit": "git-cz",
    "compile": "tsc -p ./",
    "lint": "eslint --ext .ts src/ tests/",
    "prepack": "npm run compile",
    "release": "semantic-release",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "cross-env JUNIT_REPORT_PATH=junit_report.xml nyc mocha tests/*-test.ts --reporter=mocha-jenkins-reporter",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appcelerator/titanium-editor-commons.git"
  },
  "keywords": [],
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@seadub/danger-plugin-dependencies": "^1.0.0",
    "@seadub/danger-plugin-eslint": "^2.0.0",
    "@seadub/danger-plugin-junit": "^0.3.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@types/chai": "^4.2.4",
    "@types/chai-as-promised": "^7.1.3",
    "@types/fs-extra": "^9.0.0",
    "@types/mocha": "^9.0.0",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "~12.20.0",
    "@types/semver": "^7.1.0",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "danger": "^11.0.2",
    "eslint": "^7.10.0",
    "eslint-config-axway": "^6.0.0",
    "eslint-plugin-mocha": "^9.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^12.1.5",
    "mocha": "^9.1.0",
    "mocha-jenkins-reporter": "^0.4.2",
    "mock-fs": "^5.0.0",
    "nock": "^13.0.0",
    "nyc": "^15.0.0",
    "pinst": "^2.1.6",
    "semantic-release": "^20.0.2",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "tmp": "^0.2.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "semver": "^7.1.3"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html",
      "lcov",
      "cobertura"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ]
    ]
  }
}
